{
 "name": "tth-v-pagination",
 "version": "1.0.0",
 "keywords": [
  "Vue",
  "Pagination"
 ],
 "author": "AIMEN.S.A.SASI",
 "license": "ISC",
 "description": "A Vue Pagination Component",
 "main": "dist/vue-pagination.umd.js",
 "module": "dist/vue-pagination.esm.js",
 "unpkg": "dist/vue-pagination.min.js",
 "browser": {
  "./sfc": "src/vue-pagination.vue"
 },
 "scripts": {
  "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
  "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-pagination.umd.js",
  "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-pagination.esm.js",
  "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-pagination.min.js"
 },
 "devDependencies": {
  "@rollup/plugin-buble": "^0.21.1",
  "@rollup/plugin-commonjs": "^11.0.2",
  "acorn": "^6.0.0",
  "rollup": "^1.31.0",
  "rollup-plugin-vue": "^5.1.6",
  "vue": "^2.6.11",
  "vue-template-compiler": "^2.6.11"
 }
}
